<!DOCTYPE html>
<html>
<head>
  <title>Tester</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p>
  <% if flash[:alert].present? %>
    <%= flash[:alert] %>
  <% end %>
</p>

<div class="content container">
  <%= yield %>
</div>

</body>
</html>
